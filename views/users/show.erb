<h4>Individual User</h4>

<ul>
<% @users.each do |user| %>

  <li>
    <%= @user[:name] %> is <%= @user[:id] %>
  </li>

  <li>
    This is <%= @user[:name] %>
  </li>

  <li>
    <%= @user[:name] %> email is <%= @user[:email] %>
  </li>

  <li>
    <%= @user[:name] %> address is <%= @user[:address] %>
  </li>

  <li>
    <%= @user[:name] %> password is <%= @user[:password] %>
  </li>

  <li>
    <%= @user[:name] %> credit card number is <%= @user[:cc_number] %>
  </li>

  <li>
    <%= @user[:name] %> joined us on <%= @user[:created_at] %>
  </li>
<% end %>

</ul>
